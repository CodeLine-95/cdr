.list{
  margin-top: 10px;
  padding: 150px 10px;
  background-color: #ffffff;
  height: 100vh;

}

.listItem{
  height: 50px;
  line-height: 50px;
  border-bottom: 1px solid #F3F3F3;
}
.text{
  display: block;
  height: 50px;
  line-height: 50px;
  float: left;
  color: #5B5B5B;
  margin-right: 30px;
  padding: 0 10px;
}
.input{
  display: block;
  line-height: 50px;
  height: 50px;
  float: left;
}
.btn-normal{
  padding: 0 20px !important;
  border-radius: 30px !important;
  border: 1px solid #EF8139 !important;
  color: #EF8139 !important;
}
.btn-sub{
  width: 60%;
  margin: 0 auto;
  margin-top: 60rpx;
  padding: 0 20px !important;
  border-radius: 30px !important;
  border: none;
  /* border: 1px solid #EF8139 !important; */
  color: #ffffff !important;
  background-color: #EF8139;
}